State.status === 'pending' && <p>Loading...</p>}\r\n        </AuthContext.Provider>\r\n    );\r\n}\r\n\r\nfunction useAuthState () {\r\n    const authState = useContext (AuthContext);\r\n\r\n    const isDone = authState.status === 'done';\r\n    const isAuthenticated = authState.user !== null && isDone;\r\n\r\n    //console.log ('authenticated', isAuthenticated);\r\n\r\n    return {\r\n        ...authState,\r\n        isAuthenticated: isAuthenticated,\r\n    }\r\n}\r\n\r\nexport {\r\n    AuthContext,\r\n    useAuthState,\r\n    AuthContextProvider,\r\n};\r\n","C:\\Users\\David van Heesch\\WebstormProjects\\frontend-eindopdracht\\src\\components\\signInForm\\index.js",["72","73"],"import React, {useContext, useEffect, useState} from \"react\";\r\nimport './SignInForm.css'\r\nimport { useForm } from \"react-hook-form\";\r\nimport {Link, useHistory} from \"react-router-dom\";\r\nimport axios from \"axios\";\r\nimport {AuthContext, useAuthState} from \"../../context/AuthContext\";\r\n\r\nexport default function SignInForm () {\r\n\r\n    const { login } = useContext(AuthContext);\r\n    const { isAuthenticated } = useAuthState();\r\n\r\n    const { register, handleSubmit, errors } = useForm();\r\n    const [loading, toggleLoading] = useState(false);\r\n    const [error, setError] = useState('')\r\n\r\n    const history = useHistory();\r\n\r\n    useEffect(() => {\r\n        if (isAuthenticated === true) {\r\n            history.push('/Profile');\r\n        }\r\n    }, [isAuthenticated]);\r\n\r\n        async function onSubmit(data) {\r\n            toggleLoading(true);\r\n            setError('');\r\n            console.log (data);\r\n        try {\r\n            const response = await axios.post (\r\n                'https://polar-lake-14365.herokuapp.com/api/auth/signin', {\r\n                    username: data.username,\r\n                    password: data.password,\r\n                });\r\n\r\n            //handel het inloggen aan de voorkant af in de context met de data die we binnen hebben gekregen!\r\n            login (response.data);\r\n        } catch(e) {\r\n            console.error (e);\r\n            if (e.message.includes('401')) {\r\n                setError ('Gebruikersnaam of wachtwoord is onjuist.')\r\n            } else {\r\n                setError ('Inloggen is helaas mislukt, probeer het nogmaals.')\r\n            }\r\n            //tip: als de gebruikersnaam niet bestaat of ww is verkeerd, stuurt de backend een 401\r\n        }\r\n        toggleLoading(false);\r\n\r\n    }\r\n\r\n    return <form onSubmit={handleSubmit(onSubmit)}>\r\n\r\n        <h1>Sign in</h1>\r\n        <div className=\"background-signin\">\r\n\r\n        <label htmlFor=\"username-details\">Username</label>\r\n        <input\r\n            name=\"username\"\r\n            type=\"text\"\r\n            ref={register({required: true})}\r\n        />\r\n        {errors.username && <span>This field is required</span>}\r\n\r\n        <label htmlFor=\"password-details\">Password</label>\r\n        <input\r\n            name=\"password\"\r\n            type=\"password\"\r\n            ref={register({required: true})}\r\n        />\r\n        {errors.password && <span>This field is required</span>}\r\n\r\n        <button\r\n            type=\"submit\"\r\n            className=\"form-button\"\r\n            disabled={loading}\r\n        >\r\n            {loading? 'Loading..' : 'Sign in'}\r\n        </button>\r\n        {error && <p>{error}</p>}\r\n\r\n        </div>\r\n\r\n    </form>\r\n}\r\n","C:\\Users\\David van Heesch\\WebstormProjects\\frontend-eindopdracht\\src\\components\\signUpForm\\index.js",[],"C:\\Users\\David van Heesch\\WebstormProjects\\frontend-eindopdracht\\src\\components\\header\\index.js",["74","75","76"],"C:\\Users\\David van Heesch\\WebstormProjects\\frontend-eindopdracht\\src\\components\\searchBar\\index.js",["77"],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"